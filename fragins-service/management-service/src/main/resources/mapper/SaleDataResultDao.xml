<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sinosoft.fragins.management.dao.SaleDataResultDao">
    <select id="selectByBranchNameAnddataWeidu" resultType="com.sinosoft.fragins.management.po.SaleDataResult">
        select
        *
        from sale_data_result
        where sale_name = #{branchName} and data_weidu = #{dataWeidu}
    </select>

    <delete id="deleteAll">
        delete from sale_data_result
    </delete>

</mapper>